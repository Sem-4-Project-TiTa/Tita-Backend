    "x3 = independent.copy()\n",
    "num_cols = x3.columns\n",
    "for i in num_cols:\n",
    "    x3[[i]] = (x3[[i]]-x3[[i]].min())/(x3[[i]].max()-x3[[i]].min())\n",
    "x3"
