    "x1=independent.copy()\n",
    "y1=dependent.copy()\n",
    "num_cols = x1.columns\n",
    "for i in num_cols:\n",
    "    x1[[i]] = (x1[[i]]-x1[[i]].mean())/x1[[i]].std()\n",
    "x1"
